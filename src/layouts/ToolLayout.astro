---
interface Props {
  title: string;
}

const { title } = Astro.props;

import NavBar from "../components/NavBar.astro";
import BaseLayout from "./BaseLayout.astro";
---

<BaseLayout title={title}>
  <NavBar active="none" />

  <h2 class="index-callout-main hide-on-desktop text-gradient">{title}</h2>

  <slot />
</BaseLayout>
